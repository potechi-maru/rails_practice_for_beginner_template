<h1>Users#new</h1>

<div class='container'>

<%= form_with model: @user do |f| %>
<div>
  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>
</div>
<div>
  <%= f.label :email %>
  <%= f.email_field :email, class: 'form-control' %>
</div>

<div>
  <%= f.label :password %>
  <%= f.password_field :password, class: 'form-control' %>
</div>

<div>
  <%= f.label :password_confirmartion %>
  <%= f.password_field :password_confirmartion, class: 'form-control' %>
</div>

<%= f.submit class: 'btn btn-primary' %>

<% end %>

</div>